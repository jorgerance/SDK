{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {
        "account_name": {
            "type": "string",
            "pattern": "^[1-5a-z]{2,12}$"
        },

        "name": {
            "type": "string",
            "pattern": "^[1-5a-z]{2,12}$"
        },

        "asset": {
            "type": "string",
            "pattern": "^[0-9]+\\.[0-9]+\\ [A-Z]+"
        },

        "uint32": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4294967295
        },

        "uint64": {
            "type": "integer",
            "minimum": 0,
            "maximum": 18446744073709552000
        }
    }
}
